@using Kendo.Mvc.UI
@using KendoDP2.Areas.Organizacion.Models
@using KendoDP2.Areas.Configuracion.Models

@model KendoDP2.Areas.Organizacion.Models.ColaboradorDocumentosDTO
    
         <div class="row-fluid">
            <!-- contenedor superior izquierdo: campos -->
                      
                <div class="row-fluid">
                    <div class="span3">
                        <div class="editor-label" style="width: 100%; text-align: left;">@Html.LabelFor(model => model.TipoDocumento)</div>
                    </div>
                    <div class="span4">
                        <div class="editor-field" style="width: 100%; text-align: left;">
                           @Html.TextBoxFor(model => model.TipoDocumento, new {@readonly ="readonly"})           
                 
                         
                        </div>
                    </div>
                </div>

               <div class="row-fluid">
                    <div class="span3">
                        <div class="editor-label" style="width: 100%; text-align: left;">@Html.LabelFor(model => model.Documento)</div>
                    </div>
                    <div class="span4">
                        <div class="editor-field" style="width: 100%; text-align: left;">
                           @Html.TextBoxFor(model => model.Documento, new {@readonly ="readonly"})           
                 
                         
                        </div>
                    </div>
                </div>

      
        </div>
    
